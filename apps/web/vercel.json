{
  "buildCommand": "bun run vercel-build",
  "framework": "nextjs",
  "installCommand": "bun install",
  "env": {
    "DATABASE_URL": "file:/tmp/deploy.db"
  },
  "build": {
    "env": {
      "DATABASE_URL": "file:/tmp/deploy.db"
    }
  },
  "functions": {
    "app/**/route.ts": {
      "maxDuration": 30,
      "includeFiles": [
        "prisma/deploy.db"
      ]
    },
    "app/**/route.js": {
      "maxDuration": 30,
      "includeFiles": [
        "prisma/deploy.db"
      ]
    }
  }
}
