{
  "buildCommand": "bun run vercel-build",
  "framework": "nextjs",
  "installCommand": "bun install",

  "functions": {
    "app/**/route.ts": {
      "maxDuration": 30,
      "includeFiles": [
        "prisma/deploy.db"
      ]
    },
    "app/**/route.js": {
      "maxDuration": 30,
      "includeFiles": [
        "prisma/deploy.db"
      ]
    }
  }
}
